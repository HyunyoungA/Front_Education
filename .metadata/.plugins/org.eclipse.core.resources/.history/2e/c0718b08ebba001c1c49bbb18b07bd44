		}
		function v(){
				
			var str = '';
			str += 'var num1 = Number(prompt("숫자 입력"));';//다 문자열로 쓴거
			str += 'var num2 = Number(prompt("숫자 입력"));';//다 문자열로 쓴거
			str += 'var sum = num1 + num2;';//다 문자열로 쓴거
			
			
			console.log(str);
			eval(str);
		}
		
	</script>
	
